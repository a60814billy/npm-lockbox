{
  "name": "npm-lockbox",
  "version": "0.0.1-alpha.0",
  "description": "manage and restrict npm dependencies with specific versions or date",
  "main": "src/index.ts",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register ./tests/**/*.test.ts --exit --recursive",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [
    "npm",
    "dependency",
    "lockbox"
  ],
  "engines": {
    "node": ">=10.0.0 <11.0.0"
  },
  "author": "Raccoon Li <a60814billy@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.14.0",
    "mongodb": "^2.2.21",
    "semver": "5.3.0",
    "tslint": "^4.3.0-dev.0"
  },
  "devDependencies": {
    "@types/express": "^4.0.35",
    "@types/mocha": "^2.2.38",
    "@types/mongodb": "1.0.0",
    "@types/node": "^6.0.60",
    "@types/semver": "~5.3.30",
    "mocha": "3.2.0",
    "nodemon": "1.11.0",
    "ts-node": "2.0.0",
    "tsconfig-paths": "^2.0.0",
    "typescript": "^2.1.5"
  },
  "nodemonConfig": {
    "verbose": true,
    "exec": "ts-node ./src/index.ts",
    "watch": [
      "**"
    ],
    "ext": "ts,js,json"
  }
}
